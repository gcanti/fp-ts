// @flow
import type { HKT } from './HKT'
import type { Functor } from './Functor'

export interface Profunctor<F> extends Functor<F> {
  promap<A, B, C, D, U>(f: (a: A) => B, g: (c: C) => D, fbc: HKT<F, U, B, C>): HKT<F, U, A, D>;
}

declare export var lmap: <F, A, B, U>(
  profunctor: Profunctor<F>
) => (f: (a: A) => B) => <C>(fbc: HKT<F, U, B, C>) => HKT<F, U, A, C>

declare export var rmap: <F, C, D, U>(
  profunctor: Profunctor<F>
) => (g: (c: C) => D) => <B>(fbc: HKT<F, U, B, C>) => HKT<F, U, B, D>
