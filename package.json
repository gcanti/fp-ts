{
  "name": "fp-ts",
  "version": "3.0.0-alpha.25",
  "description": "Functional programming in TypeScript",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "types": "./types/index.d.ts"
    },
    "./*": {
      "require": "./cjs/*.js",
      "import": "./esm/*.js",
      "types": "./types/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./types/*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" \"scripts/**/*.ts\"",
    "prettier": "prettier --list-different \"./{src,test,examples,scripts}/**/*.ts\"",
    "fix-prettier": "prettier --write \"./{src,test,examples,scripts}/**/*.ts\"",
    "circular": "madge --circular --ts-config ./tsconfig.build-cjs.json --extensions ts ./src",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build-esm.json && tsc -p ./tsconfig.build-cjs.json && tsc -p ./tsconfig.build-types.json && ts-node scripts/build3",
    "postbuild": "prettier --loglevel=silent --write \"./dist/**/*.ts\"",
    "prepublishOnly": "ts-node scripts/pre-publish",
    "doctoc": "doctoc README.md --title \"**Table of contents**\"",
    "mocha": "mocha -r ts-node/register test/*.ts",
    "dtslint": "dtslint dtslint",
    "docs": "docs-ts",
    "prerelease": "npm run build",
    "release": "ts-node scripts/release",
    "vitest": "vitest run",
    "test": "npm run circular && npm run lint && npm run prettier && npm run dtslint && npm run vitest && npm run docs",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/fp-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/fp-ts/issues"
  },
  "homepage": "https://github.com/gcanti/fp-ts",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/glob": "^7.1.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^12.6.8",
    "@types/prettier": "1.10.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/coverage-c8": "^0.23.4",
    "benchmark": "2.1.4",
    "docs-ts": "^0.6.10",
    "doctoc": "^1.4.0",
    "dtslint": "github:gcanti/dtslint",
    "eslint": "^8.23.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "fast-check": "^3.1.3",
    "glob": "^7.1.6",
    "madge": "^4.0.2",
    "mocha": "^5.2.0",
    "prettier": "^2.7.1",
    "rimraf": "2.6.2",
    "ts-morph": "^9.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.2",
    "vitest": "^0.23.4"
  },
  "tags": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "keywords": [
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ]
}
